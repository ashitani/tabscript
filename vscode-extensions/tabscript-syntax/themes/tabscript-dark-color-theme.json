{
    "name": "TabScript Dark",
    "type": "dark",
    "colors": {
        "editor.background": "#1E1E1E",
        "editor.foreground": "#D4D4D4"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment.line.number-sign.tabscript", "comment.line.double-slash.tabscript"],
            "settings": {
                "foreground": "#6A9955"
            }
        },
        {
            "name": "Metadata",
            "scope": ["meta.metadata.tabscript"],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "Section",
            "scope": ["meta.section.tabscript", "meta.triplet.tabscript"],
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Chord Name",
            "scope": ["tab.chord-name.tabscript"],
            "settings": {
                "foreground": "#D7BA7D"
            }
        },
        {
            "name": "String Number",
            "scope": ["tab.string-number.tabscript"],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "Fret Number",
            "scope": ["tab.fret-number.tabscript"],
            "settings": {
                "foreground": "#FF79C6"
            }
        },
        {
            "name": "Note Length",
            "scope": ["tab.note-length.tabscript"],
            "settings": {
                "foreground": "#4EC9B0"
            }
        }
    ]
} 